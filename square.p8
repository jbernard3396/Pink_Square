//game_player
function create_game_player()
 gp = {}
 gp.workers = {}
 add(gp.workers, create_game_controler())
 add(gp.workers, create_timer())
 add(gp.workers, create_player())
 add(gp.workers, create_score_keeper())
 gp.update = function()
  for item in all(gp.workers) do
   item.update()
  end
 end
 gp.draw = function()
  for item in all(gp.workers) do
   item.draw()
  end
 end
 gp.instantiate = function()
  for item in all(gp.workers) do
   if item.instantiate then
	   item.instantiate()
	  end
  end
 end
 return gp
end

function create_game_controler()
	local this = {}
  
 this.draw = function()
 end
 
 this.update = function()
 end
 game_controler = this
 return this
end

function create_timer()
 local this = {}
 this.instantiate = function()
 end
 this.external_time = 0
 this.internal_time = 0
 this.turn_length = 1
 this.draw = function() 
  --print(this.external_time, 120, 0, cm.get('secondary_1'))
 end
 this.update = function()
  this.internal_time += 1
  if this.internal_time >= this.turn_length then
   this.internal_time = 0
   this.external_time += 1	
   player_manager.change_size()
  end
 end
 timer = this
 return this
end

function create_player()
 local this = {}
 this.getting_smaller = true
 this.side = 120
 this.max_length = 120
 this.min_length = 10
 this.perfect_width = 3
 this.update = function()
 	if btnp(❎) then
 	 timer.turn_length -= .1
		 timer.turn_length = max(timer.turn_length, 1)
 		this.reverse()
 		this.score_points()
 	end
 	this.check_dead()
 	this.bounce()
 end
 this.draw = function() 
 	--min
 	rectfill(64-this.min_length/2, 64-this.min_length/2, 64+this.min_length/2, 64+this.min_length/2, 14)
 	rect(64-this.min_length/2-1, 64-this.min_length/2-1, 64+this.min_length/2, 64+this.min_length/2, 10)
 	
 	--player
 	rect(64-this.side/2, 64-this.side/2, 63+this.side/2, 63+this.side/2, 7)
 	
 end
 this.check_dead = function()
 	if this.side <= this.min_length then
 	 if score_keeper.score == 0 then
 	 	gm.state = 'pause'
 	 else
 		
 		 gm.result = 'death'
 		 gm.death_reason = get_death_reason()
 		 gm.state = 'over'
 	 end
 	end
 end
 this.bounce = function()
 	if this.side >= this.max_length	then
 		this.getting_smaller = true
 	end
 end
 this.change_size = function()
  if this.getting_smaller then
   this.side -= 2
  else 
  	this.side += 2
  end
 end
 this.reverse = function()
 	if this.getting_smaller then
 		this.min_length = this.side-1
		 this.getting_smaller = false
 	end

 end
 this.score_points = function()
 	score_keeper.increment_score(1)
 end
 player_manager = this
 return this
end

function create_score_keeper()
 local this = {}
 this.score = 0
 this.hs = 0
 this.draw = function()
  print(flr(this.score),0,0,cm.get('secondary_1'))
 end
 this.increment_score = function(score)
  this.score += score or 1
  if this.score > highscore then
  	highscore = this.score
  end
 end
 this.update = function()

 end
 score_keeper = this
 return this
end
