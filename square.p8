//system core loop
function _init()
 if score_keeper then
 	hs = score_keeper.hs
 end
 create_game_manager()
 create_color_manager()
 if hs then
 	score_keeper.hs = hs
 end
 
end

function _update60()
 gm.update()
 cm.update()
end

function _draw()
 cls()
 gm.draw()
 cm.draw()
end
