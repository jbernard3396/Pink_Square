function get_death_reason()
	death_reasons = {
	'too slow',
	'monitor turned off',
	'windows update',
	'had to wash your hair',
	'panicked',
	'forgot how to play',
	'abducted by aliens',
	'forgot to blink',
	'bird pooped on you',
	'keyboard broke',
	'cpu over heated',
	'got hacked',
	'distracted by nachos',
	'got bored',
	'died of dysentery',
	'unlucky',
	'time fluctuation',
	'brain exploded',
	'its a trap'
	}
	return pick(death_reasons)
end
