//game

function create_game_manager()
 gm = {}
 gm.state = 'menu'
 gm.menu_manager = create_menu_manager()
 gm.game_player = create_game_player()
 gm.start_game = function()
  gm.game_player.instantiate()
  gm.state = 'game'
 end
 gm.update = function()
  if gm.state == 'menu' then
   gm.menu_manager.update()
  elseif gm.state == 'game' then
   gm.game_player.update()
  elseif gm.state == 'pause' then
   if btnp(❎) then
    player_manager.getting_smaller = false
    gm.state = 'game'
   end
  end
 end
 gm.draw = function()
  if gm.state == 'menu' then
   gm.menu_manager.draw()
  elseif gm.state == 'game' then
   gm.game_player.draw()
  elseif gm.state == 'pause' then
   print('press ❎ before the       square', 0, 34, 13) 
   print('touches the      square', 12, 42, 13)
   print('white', 80, 34, 7)
   print('pink', 60, 42, 14)
   gm.game_player.draw()
  elseif gm.state == 'over' then
   gm.game_player.draw()
   if score_keeper.score > score_keeper.hs then
    score_keeper.hs = score_keeper.score
   end
   if gm.result == 'death' then
    print('game over', 12, 12, 13) 
    print('reason: '..gm.death_reason, 12, 20, 13) 
    print('score: '..score_keeper.score, 12, 28, 13)
    --print('ticks survives: '..timer.external_time, 12, 36, 7)
    print('press ⬆️ to try again', 12, 52, 13)
	   print('press ⬇️ to return to menu', 12, 60, 13)
	   if btnp(⬇️) then
	   	_init()
	   end
	   if btnp(⬆️) then
	    _init()
	   	gm.state = 'game'
	   end
   end
  end
 end
end


